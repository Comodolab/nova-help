{
    "name": "comodolab/nova-help",
    "description": "A Laravel Nova help field.",
    "keywords": [
        "laravel",
        "nova",
        "help"
    ],
    "authors": [
        {
            "name": "Alessandro Benoit",
            "email": "alessandro@comodolab.it"
        }
    ],
    "license": "MIT",
    "require": {
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0",
        "php": ">=7.2.5"
    },
    "autoload": {
        "psr-4": {
            "Comodolab\\Nova\\Fields\\Help\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Comodolab\\Nova\\Fields\\Help\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
